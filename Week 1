function setup() {
  createCanvas(500,500)
  background(240);
}

function draw() {
  line(250,250,50,500)
  line(250,250,470,500)
  line(300,300,10,500)
  line(300,300,490,500)
  line(350,350,500,500)
  line(350,350,90,500)
  line(200,200,30,500)
  line(200,200,450,500)
  line(150,150,70,500)
  line(150,150,430,500)
  line(100,100,80,500)
  line(100,100,410,500)
  line(50,50,60,500)
  line(50,50,390,500)
  line(0,0,60,500)
  line(0,0,370,500)
  
}
